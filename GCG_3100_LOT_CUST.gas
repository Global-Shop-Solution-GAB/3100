Program.Sub.Main.Start
F.Intrinsic.Control.SetErrorHandler("Main_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)
V.Local.sSql.Declare(String)

F.odbc.Connection!conx.OpenConnection(V.Ambient.PDSN,V.Ambient.PUser,V.Ambient.PPass)

'if there is no information or first time create default cust for custom
V.Local.sSql.Set("SELECT CUST, NAME,PATH FROM GCG_3100_LOT_CUST WHERE CUST='000000'")
F.ODBC.connection!conx.OpenLocalRecordsetRW("rstCust",V.Local.sSql)
F.Intrinsic.Control.If(V.ODBC.conx!rstCust.EOF,=,True)
	F.ODBC.conx!rstCust.AddNew
	F.ODBC.conx!rstCust.Set!CUST("000000")
	F.ODBC.conx!rstCust.Set!NAME("C")
	F.ODBC.conx!rstCust.Set!PATH("")
	F.ODBC.conx!rstCust.Update
F.Intrinsic.Control.Else
	'this has been ran before, check to see if all or custom is selected and set the radio button and path selected.
	F.Intrinsic.Control.If(V.ODBC.conx!rstCust.FieldValTrim!NAME,=,"C")
		Gui.F_LOT_CUST.optCustom.Value(1)
		Gui.F_LOT_CUST.optTrue.Value(0)
		Gui.F_LOT_CUST.txtAllPath.Text(V.ODBC.conx!rstCust.FieldValTrim!PATH)
	F.Intrinsic.Control.Else
		Gui.F_LOT_CUST.optCustom.Value(0)
		Gui.F_LOT_CUST.optTrue.Value(1)
		Gui.F_LOT_CUST.txtAllPath.Text(V.ODBC.conx!rstCust.FieldValTrim!PATH)
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
F.ODBC.conx!rstCust.Close

Gui.F_LOT_CUST..Show

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("Main_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GCG_3100_LOT_CUST.gas",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(F_end)
Function.Intrinsic.Control.EndIf

Program.Sub.Main.End

